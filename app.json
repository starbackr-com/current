{
  "expo": {
    "name": "Current",
    "slug": "current",
    "version": "0.1.4",
    "orientation": "portrait",
    "backgroundColor": "#18181b",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "jsEngine": "hermes",
    "owner": "lightningdigitalentertainment",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181b"
    },
    "scheme": "nostr",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/04522974-aa48-4b3e-ad26-71be0648d73c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "io.getcurrent.current",
      "buildNumber": "6",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:getcurrent.io"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.getcurrent.io",
              "pathPrefix": "/link"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "io.getcurrent.current",
      "versionCode": 46,
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/current-android-logo-transparent.png",
      "color": "#faa200",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Current App"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "04522974-aa48-4b3e-ad26-71be0648d73c"
      }
    },
    "plugins": [
      "expo-localization"
    ],
    "hooks": {
      "postPublish": [
        {
          "config": {
            "organization": "lightning-digital-entertainmen",
            "project": "current"
          }
        }
      ]
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
